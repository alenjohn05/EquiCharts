{
  "name": "equicharts-widget",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-main": "tsc && vite build",
    "build-dts": "dts-bundle-generator --no-banner true --umd-module-name equichartswidget -o dist/index.d.ts src/index.ts",
    "build": "npm run build-main && npm run build-dts",
    "preview": "vite preview",
    "build:core": "npm run build --workspace equicharts",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,json,md}\""
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "rollup": "^4.20.0",
    "solid-js": "^1.8.19"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "equicharts": "workspaces/equicharts",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.0",
    "vite-plugin-solid": "^2.10.2"
  },
  "workspaces": [
    "equicharts"
  ]
}
